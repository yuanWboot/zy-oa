<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zy.oa.mapper.NoticeMapper">
    <insert id="insert" parameterType="com.zy.oa.entity.Notice"
            useGeneratedKeys="true" keyProperty="noticeId" keyColumn="notice_id">
        INSERT INTO `zy_oa`.`sys_notice`( `receiver_id`, `content`, `create_time`)
        VALUES ( #{receiverId},  #{content},  #{createTime});
    </insert>
</mapper>